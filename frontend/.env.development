# Development Environment Configuration
# These variables are used when running the app in development mode (npm run dev)

# Backend API URL for development (localhost)
VITE_API_URL=http://localhost:3001

# WebSocket URL for development (localhost)
VITE_WS_URL=ws://localhost:3001

# Environment identifier
VITE_APP_ENV=development

# API timeout in milliseconds
VITE_API_TIMEOUT=600000

# Number of retry attempts for failed requests
VITE_API_RETRIES=3

# WebSocket reconnection settings
VITE_WS_RECONNECT_DELAY=2000
VITE_WS_MAX_RECONNECT_ATTEMPTS=5