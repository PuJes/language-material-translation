# Environment Variables Example
# Copy this file to .env.development or .env.production and update the values

# Backend API URL (required)
VITE_API_URL=http://localhost:3001

# WebSocket URL (required)
VITE_WS_URL=ws://localhost:3001

# Environment identifier (optional)
VITE_APP_ENV=development

# API timeout in milliseconds (optional, default: 600000)
VITE_API_TIMEOUT=600000

# Number of retry attempts for failed requests (optional, default: 3)
VITE_API_RETRIES=3

# WebSocket reconnection delay in milliseconds (optional, default: 2000)
VITE_WS_RECONNECT_DELAY=2000

# Maximum WebSocket reconnection attempts (optional, default: 5)
VITE_WS_MAX_RECONNECT_ATTEMPTS=5